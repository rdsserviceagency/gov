<%- include("partials/header"); -%>

                <!-- End of Topbar -->

                <!-- Begin Page Content -->
                <div class="container-fluid">
                    <div class="col-xl-12 col-lg-12">
                        <div class="card shadow mb-4">
                            <!-- Card Header - Dropdown -->
                            <div
                                class="card-header py-3 d-flex flex-row align-items-center justify-content-between" style="background-color: #2F2E41;">
                                <h6 class="m-0" style="color: #fff; font-weight: 700;">दस्तावेज</h6>
                            </div>
                            <!-- Card Body -->
                            <div class="card-body">
                                <form action="/admin/sub/<%=dname%>/update" method="post">
                                <div class="form-group">
                                    <label for="exampleInputPassword1" style="color: #2F2E41;"> विलेख चुने<sup style="color: red;">*</sup></label>
                                    <select name="विलेख" id="exampleInputPassword1" disabled  required class="form-control" aria-placeholder="Select Status">
                                        <% for(let i=0;i<parray.length;i++){ %>
                                            <%if(dname==parray[i].name){%>}
                                               <option value="<%=parray[i].name%>"  selected><%=parray[i].name%></option>
                                            <%}%>
                                            <%if(dname!=parray[i].name){%>}
                                               <option value="<%=parray[i].name%>" ><%=parray[i].name%></option>
                                            <%}%>
                                        <%}%>
                                    </select>
                                </div>
                                    <div class="form-group">
                                        <label for="exampleInputEmail1" style="color: #2F2E41;">दस्तावेज का प्रकार <sup style="color: red;">*</sup></label>
                                        <input type="text" readonly value="<%=name%>" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name="name">
                                    </div>

                                    <div class="form-group">
                                        <label for="exampleInputEmail1" style="color: #2F2E41;">दस्तावेज का विवरण <sup style="color: red;">*</sup></label>
                                        <input type="text" required value="<%=description%>" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name="description">
                                    </div>
                                
                                    <div class="form-group">
                                        <label for="exampleInputEmail1" style="color: #2F2E41;">शुल्क का प्रकार <sup style="color: red;">*</sup></label>
                                        <select name="Type" id="exampleInputEmail1" required class="form-control" aria-placeholder="Select Status">
                                            <%if(Type=="Fixed"){%>}
                                                <option value="Fixed" selected>Fixed</option>
                                                <option value="Percentage">Percentage</option>
                                                <option value="Per Feet">Per Feet</option>
                                            <%}%>
                                            <%if(Type=="Percentage"){%>}
                                                <option value="Fixed">Fixed</option>
                                                <option value="Percentage" selected>Percentage</option>
                                                <option value="Per Feet">Per Feet</option>
                                            <%}%>
                                            <%if(Type=="Per Feet"){%>}
                                                <option value="Fixed">Fixed</option>
                                                <option value="Percentage">Percentage</option>
                                                <option value="Per Feet" selected>Per Feet</option>
                                            <%}%>
                                        </select>
                                    </div>

                                    <div class="form-group">
                                        <label for="exampleInputEmail1" style="color: #2F2E41;">स्टाम्प शुल्क<sup style="color: red;">*</sup></label>
                                        <input type="number" value="<%=Stamp%>" step="0.01" required class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name="Stamp">
                                    </div>


                                    <div class="form-group">
                                        <label for="exampleInputEmail1" style="color: #2F2E41;">उपकर शुल्क<sup style="color: red;">*</sup></label>
                                        <input type="number" value="<%=Upkar%>" step="0.01" required class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name="Upkar">
                                    </div>

                                    <div class="form-group">
                                        <label for="exampleInputEmail1" style="color: #2F2E41;">जनपद शुल्क<sup style="color: red;">*</sup></label>
                                        <input type="number" value="<%=Janpad%>" step="0.01" required class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name="Janpad">
                                    </div>

                                    <div class="form-group">
                                        <label for="exampleInputEmail1" style="color: #2F2E41;">पंजीयन शुल्क  <sup style="color: red;">*</sup></label>
                                        <input type="number" value="<%=panjikar%>" step="0.01" required class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name="panjikar">
                                    </div>

                                    <!--Image upload-->
                                    <button type="submit" class="btn btn-primary" style="background:#2F2E41; color: #fff; font-weight: 700;">Submit</button>
                                </form>
                            </div>
                        </div>
                    </div>
                    
                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
         
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Logout Modal-->
    <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
        aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
                    <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
                <div class="modal-footer">
                    <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
                    <a class="btn btn-primary" href="login.html">Logout</a>
                </div>
            </div>
        </div>
    </div>

    <!-- Bootstrap core JavaScript-->
    <script src="/vendor/jquery/jquery.min.js"></script>
    <script src="/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="/vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="/js/sb-admin-2.min.js"></script>

    <!-- Page level plugins -->
    <script src="/vendor/chart.js/Chart.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="/js/demo/chart-area-demo.js"></script>
    <script src="/js/demo/chart-pie-demo.js"></script>

</body>

</html>