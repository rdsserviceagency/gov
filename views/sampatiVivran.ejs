            // Get field values in the section
            const fields = section.querySelectorAll(".input");
            fields.forEach(function (field) {
                const fieldName = field.name;
             
                const fieldValue = field.value || null;
              
                sectionData[fieldName] = fieldValue;
            });

            // Get Vriksh data
