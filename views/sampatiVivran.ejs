        nyalyAdesh.style.display = "none";
        nyalyAdeshFields.forEach((nyalyAdeshField) => {
            nyalyAdeshField.required = false;
            nyalyAdeshField.value = ""
        })
    }
    function shownyalyAdesh() {
        nyalyAdesh.style.display = "block";
        nyalyAdeshFields.forEach((nyalyAdeshField) => {
            nyalyAdeshField.required = true;
        })
    }
    function hideSource() {
        sourceForm.style.display = "none";
        sourceFormFields.forEach((sourceFormField) => {
            sourceFormField.required = false;
            sourceFormField.value = ""
        })
    }
    function showFieldExcept(hideFields) {
        sourceForm.style.display = "block";
        sourceFormFields.forEach((sourceFormField) => {
            if (hideFields.includes(sourceFormField)) {
                document.querySelector(`label[for='${sourceFormField.id}']`).style.display = "none";
                sourceFormField.style.display = "none";
                sourceFormField.required = false;
                sourceFormField.value = ""
            } else {
                sourceFormField.style.display = "block";
                document.querySelector(`label[for='${sourceFormField.id}']`).style.display = "block";
                sourceFormField.required = true;
            }
        })
    }
