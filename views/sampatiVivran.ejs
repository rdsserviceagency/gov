                    const fieldName = field.name;

                    const fieldValue = field.value || null;
                    vriksh[fieldName] = fieldValue;
                });
                vrikshData.push(vriksh);
            });

            // Get Asset data
            const assetSections = section.querySelectorAll(".asset-section");
            const assetData = [];
            assetSections.forEach(function (assetSection) {
                const asset = {};
                const assetFields = assetSection.querySelectorAll(".input");
                assetFields.forEach(function (field) {
                    const fieldName = field.name;
                    const fieldValue = field.value || null;
                    asset[fieldName] = fieldValue;
                });
                assetData.push(asset);
            });

            sectionData.vrikshData = vrikshData;
            sectionData.assetData = assetData;

            formData.push(sectionData);
        });

