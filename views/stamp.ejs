        /* styles.css */
      /* styles.css */
      body {
          font-family: Arial, sans-serif;
          margin: 0;
      }
      
      /* Style the pop-up container */
      .popup-container {
          display: none;
          position: fixed;
          top: 0;
          left: 0;
          width: 100%;
          height: 100%;
          background: rgba(0, 0, 0, 0.5);
      }
      
      /* Style the pop-up content */
      .popup-content {
          background: #fff;
          max-width: 40%; /* Adjust max-width as needed */
          margin: 5% auto;
          padding: 20px;
          border-radius: 5px;
          box-shadow: 0 0 10px rgba(0, 0, 0, 0.3);
          text-align: center; 
      }
      
      /* Style the close button */
      .close-btn {
          float: right;
          font-size: 40px;
          cursor: pointer;
      }
      
      /* Make the pop-up content responsive */
      @media (max-width: 600px) {
          .popup-content {
              max-width: 90%;
          }
      }
      
      /* Style the image inside the pop-up */
      #popup-image {
          max-width: 100%;
          max-height: 100%;
          object-fit: cover;
          border-radius: 5px;
      }
      
      </style>
